/* 用户ide数据 */

.head {
  height: 170rpx;
}

.head-img {
  width: 116rpx;
  height: 116rpx;
  background-color: pink;
  border-radius: 50%;
  margin: 14rpx 12rpx 14rpx 40rpx;
  float: left;
}

.head-img image {
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
  overflow: hidden;
}

.logs-top {
  height: 36rpx;
  width: 450rpx;
  float: left;
  margin-top: 34rpx;
}

.logs-name {
  width: 250rpx;
  font-size: 32rpx;
  color: #313131;
  font-weight: 700;
  float: left;
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.name-level {
  height: 36rpx;
  background-color: #333;
  border-radius: 99999rpx;
  font-size: 26rpx;
  color: #fff;
  float: left;
  text-align: center;
  line-height: 36rpx;
  padding: 0 18rpx;
  margin-left: 20rpx;
}

.logs-id {
  margin-top: 76rpx;
  font-size: 26rpx;
  color: #000;
}

.logs-mi {
  width: 290rpx;
  float: left;
}

.loge-besom {
  width: 80rpx;
  height: 80rpx;
  float: right;
  margin: 32rpx 24rpx;
}

.logs-login {
  padding-top: 48rpx;
}

/* 数量数据 */

.logs-data {
  display: flex;
  margin-bottom: 58rpx;
}

.data-record01, .data-record02, .data-record03 {
  width: 118rpx;
  height: 86rpx;
  flex: 1;
}

.record-top {
  font-size: 36rpx;
  color: #313131;
  text-align: center;
  transform: translateY(-6rpx);
}

.record-bottom {
  font-size: 26rpx;
  color: #313131;
  text-align: center;
  transform: translateY(10rpx);
}

/* 我的优惠券 */

.logs-coupon {
  width: 710rpx;
  height: 96rpx;
  line-height: 96rpx;
  background-color: #f0dcac;
  border-radius: 10rpx;
  margin: 0 auto;
}

.coupon-lf {
  height: 44rpx;
  width: 44rpx;
  margin: 26rpx 8rpx 26rpx 34rpx;
  float: left;
}

.coupon-pl {
  font-size: 28rpx;
  color: #7b5533;
  float: left;
}

.coupon-pr {
  font-size: 28rpx;
  color: #7b5533;
  float: right;
}

.coupon-rt {
  width: 14rpx;
  height: 24rpx;
  float: right;
  margin: 36rpx 26rpx 36rpx 12rpx;
}

.logs-indent {
  margin-top: 24rpx;
  margin-bottom: 24rpx;
  display: flex;
}

.indent-lf {
  font-size: 30rpx;
  color: #313131;
  margin-left: 20rpx;
  flex: 1;
}

.indent-rt {
  font-size: 28rpx;
  color: #868686;
}

.logs-indent image {
  width: 14rpx;
  height: 24rpx;
  margin: 8rpx 20rpx 10rpx 8rpx;
}

.indent-br {
  width: 730rpx;
  height: 1rpx;
  background-color: #f0f0f0;
  margin: 26rpx 0 48rpx 20rpx;
}

/* 订单状态 */

.logs-state {
  display: flex;
  text-align: center;
  margin-bottom: 54rpx;
}

.state-content01, .state-content02, .state-content03, .state-content04 {
  flex: 1;
}

.content01-img, .content02-img {
  width: 44rpx;
  height: 40rpx;
  /* margin: 0 auto; */
}

.content03-img {
  width: 54rpx;
  height: 40rpx;
}

.content04-img {
  width: 44rpx;
  height: 40rpx;
}

.content01-p {
  font-size: 26rpx;
  color: #313131;
}

/* 其他功能 */

.logs-function-br {
  height: 20rpx;
  background-color: #f4f4f4;
}

.logs-function {
  width: 750rpx;
  height: 88rpx;
  /* background-color: pink; */
  border-bottom: 1rpx solid #f0f0f0;
  margin-left: 20rpx;
  line-height: 88rpx;
}

.function-img01 {
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin: 30rpx 14rpx 0 0;
}

.function-p {
  font-size: 28rpx;
  color: #191919;
  float: left;
}

.function-rp {
  font-size: 28rpx;
  color: #ff1c1c;
  float: left;
}

.function-rt {
  width: 14rpx;
  height: 22rpx;
  margin-right: 40rpx;
  float: right;
  margin-top: 33rpx;
}

.function-no {
  border-bottom: none;
}

/* 底部水印 */

.bottom-bg {
  width: 750rpx;
  height: 128rpx;
  background-color: #f4f4f4;
  text-align: center;
}

.bottom-bg image {
  width: 100rpx;
  height: 32rpx;
  margin-top: 32rpx;
}

.bottom-bg view {
  font-size: 20rpx;
  color: #acacac;
}

.unlogin-view {
  position: absolute;
  left: 0rpx;
  top: 310rpx;
  background: none;
  border: none;
  width: 100%;
  height: 60%;
}
